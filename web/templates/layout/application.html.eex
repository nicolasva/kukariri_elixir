    <script src="/js/vendors/jquery_file_upload/JavaScript-Canvas-to-Blob/canvas-to-blob.min.js"></script>
    <script src="/js/bootstrap/bootstrap.min.js"></script>
    <script src="/js/vendors/jquery_file_upload/javascript-Blueimp-Gallery/jquery.blueimp-gallery.min.js"></script>
