    <script src="/js/vendors/jquery_file_upload/jquery.fileupload-video.js"></script>
    <script src="/js/vendors/jquery_file_upload/jquery.fileupload-validate.js"></script>
    <script src="/js/vendors/jquery_file_upload/jquery.fileupload-ui.js"></script>
    <script src="/js/vendors/lodash/lodash.js"></script>
    <script src="/js/vendors/fullcalendar/fullcalendar.min.js"></script>
